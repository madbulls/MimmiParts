type ALPHA

### Latin-on-thai ###

### ROW 1 ###

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
    alt:				none
    shift+alt, capslock+alt:            none
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
    alt:				'\u0E20'
    shift+alt, capslock+alt:            '\u0E20'
}

